<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteCond %{HTTP_HOST} ^laith-alobaidi.com$ [NC,OR]
    RewriteCond %{HTTP_HOST} ^www.laith-alobaidi.com$
    RewriteCond %{REQUEST_URI} !userWebsite/
    RewriteRule (.*) /userWebsite/$1 [L]
    RewriteCond %{HTTP_HOST} ^api.laith-alobaidi.com$ [NC,OR]
    RewriteCond %{HTTP_HOST} ^www.api.laith-alobaidi.com$
    RewriteCond %{REQUEST_URI} !^public
    RewriteRule ^(.*)$ public/$1 [L]
    RewriteCond %{HTTP_HOST} ^admin.laith-alobaidi.com$ [NC,OR]
    RewriteCond %{HTTP_HOST} ^www.admin.laith-alobaidi.com$
    RewriteCond %{REQUEST_URI} !^adminPanel
    RewriteRule ^(.*)$ adminPanel/$1 [L]
</IfModule>
